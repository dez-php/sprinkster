<script type="text/javascript">
	var height = parseInt($('body').css('padding-bottom'));
	$('body').find('>*').each(function() {
		height += $(this).outerHeight(true);
		pt = $(this).css('padding-top');
		if(pt) { height += parseInt(pt); }
		pb = $(this).css('padding-bottom');
		if(pb) { height += parseInt(pb); }  
	});
	height = Math.min(height, window.screen.availHeight||window.screenTop||window.screenY);
	width = Math.min(window.screen.availWidth||window.screenLeft||window.screenX||800,800);
	window.resizeTo(width,height);
	$('.event-window-close').click(function() {
		window.close();
	});
</script>		
